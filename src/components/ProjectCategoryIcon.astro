---
interface Props {
  category: string;
}

const { category } = Astro.props;
---

{category == "Unity Packages" && <i class="fa-brands fa-unity" />}
{category == "Libraries" && <i class="fa-regular fa-file-code" />}
{category == "Websites" && <i class="fa-solid fa-globe" />}
{category == "Minecraft" && <i class="fa-solid fa-cubes" />}
{category == "Desktop Apps" && <i class="fa-regular fa-window-maximize" />}
